{
  "name": "MyExtensions",
  "version": "1.0",
  "description": "Helper extensions",
  "devtools_page": "devtools.html",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["./js/sites/youtube/styles.css"],
      "js": ["./js/sites/youtube/youtube.js"]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}